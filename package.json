    "dexie": "^4.0.1",
    "events": "^3.3.0",
    "nanoid": "^5.0.6",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-gtm-module": "^2.0.11",
    "react-redux": "^9.1.0",
    "react-router-dom": "^6.22.3",
    "reflect-metadata": "^0.2.2",
    "socket.io-client": "^4.7.5",
    "string-to-color": "^2.2.2",
    "styled-components": "^6.1.8",
    "throttle-debounce": "^5.0.0",
    "tsyringe": "^4.8.0"
